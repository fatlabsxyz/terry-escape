{
  "name": "client",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/client/index.js",
      "types": "./dist/client/index.d.ts"
    },
    "./cli": {
      "import": "./dist/cli/index.js",
      "types": "./dist/cli/index.d.ts"
    },
    "./init": {
      "import": "./dist/client/init.js",
      "types": "./dist/client/init.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "directories": {
    "src": "src"
  },
  "files": [
    "dist/*",
    "package.json",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p ./tsconfig.json --outDir dist",
    "client": "node ./dist/client/index.js",
    "cli": "node ./dist/client/cli.js",
    "test": "vitest run --config vitest.config.ts --passWithNoTests",
    "andres": "node --trace-uncaught ./dist/client/cli.js 'http://127.0.0.1:2448' 'Andres' 0 | tee -a /tmp/game_0 /tmp/andres.log",
    "barto": "node --trace-uncaught ./dist/client/cli.js 'http://127.0.0.1:2448' 'Bartolomeo' 0 | tee -a /tmp/game_0 /tmp/barto.log",
    "felipe": "node --trace-uncaught ./dist/client/cli.js 'http://127.0.0.1:2448' 'Felipe' 0 | tee -a /tmp/game_0 /tmp/felipe.log",
    "pedro": "node --trace-uncaught ./dist/client/cli.js 'http://127.0.0.1:2448' 'Pedro' 0 | tee -a /tmp/game_0 /tmp/pedro.log",
    "juan": "node --trace-uncaught ./dist/client/cli.js 'http://127.0.0.1:2448' 'Juan' 0 | tee -a /tmp/game_0 /tmp/juan.log"
  },
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "eventemitter3": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^5.0.5"
  }
}
